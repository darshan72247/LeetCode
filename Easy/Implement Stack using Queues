class MyStack {

    private MyStackNode head;
    private MyStackNode tail;
    
    /** Initialize your data structure here. */
    public class MyStackNode{
      int val;
      MyStackNode next;
      MyStackNode() {}  
      MyStackNode(int val) { this.val = val; }
    }
    
    MyStack(){
        head = new MyStackNode();
        tail = head;
    }
    
    /** Push element x onto stack. */
    public void push(int x) {
        MyStackNode add = new MyStackNode(x);
        add.next = tail;
        tail = add;
    }
    
    /** Removes the element on top of the stack and returns that element. */
    public int pop() {
        int ans = tail.val;
        tail = tail.next;
        return ans;
        
    }
    
    /** Get the top element. */
    public int top() {
        int ans = tail.val;
        return ans;
    }
    
    /** Returns whether the stack is empty. */
    public boolean empty() {
        return head == tail;
    }
}

/**
 * Your MyStack object will be instantiated and called as such:
 * MyStack obj = new MyStack();
 * obj.push(x);
 * int param_2 = obj.pop();
 * int param_3 = obj.top();
 * boolean param_4 = obj.empty();
 */
