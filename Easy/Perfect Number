class Solution {
    public boolean checkPerfectNumber(int num) {
        //Time complexity :- O(n)
        //try to implemet the time complexity O(sqrt(n))
        
        int result = 0;
        for(int i=1;i<=num/2;i++){
            if(num % i == 0){
                result += i;
            }
        }
        return result==num ;
    }
}
